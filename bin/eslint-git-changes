#!/usr/bin/env node
'use strict';

const argv = require('yargs')
    .usage('Usage: $0 [options]')
    .alias('c', 'cached')
    .describe('c', 'Check cached (staged) changes')
    .describe('json', 'Include JSON')
    .describe('jsx', 'Include JSX')
    .help('h')
    .alias('h', 'help')
    .argv;

const cmd = require('..');
cmd(argv);
